{
  "name": "tristris",
  "version": "0.1.0-alpha.3",
  "description": "Yet another Tetris clone",
  "main": "index.js",
  "scripts": {
    "clean": "gulp clean",
    "build:debug": "NODE_ENV=debug gulp build",
    "build:prod": "NODE_ENV=production gulp build",
    "serve:debug": "NODE_ENV=debug gulp serve",
    "serve:prod": "npm run clean && npm run build:prod && python3 -m http.server --directory=build 4141",
    "build": "npm run build:prod",
    "start": "npm run serve:debug"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tristan00b/tristris"
  },
  "keywords": [
    "Tetris",
    "WebGL",
    "JavaScript",
    "HTML5",
    "Game"
  ],
  "author": "J. Tristan Bayfield",
  "license": "GPL-3.0",
  "type": "module",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-proposal-private-methods",
      "@babel/plugin-proposal-private-property-in-object",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "plugins": [
      "@babel"
    ],
    "env": {
      "browser": true,
      "node": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/eslint-plugin": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-do-expressions": "^7.14.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.0",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-throw-expressions": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.14",
    "browserify": "^17.0.0",
    "del": "^6.0.0",
    "eslint": "^7.27.0",
    "fibers": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-git": "^2.10.1",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulplog": "^1.0.0",
    "sass": "^1.43.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
